{
  "name": "hltv-go",
  "version": "1.0.0",
  "description": "The unofficial HLTV Node.js API special for GoStars",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.release.json",
    "start": "ts-node src/playground",
    "test": "jest --runInBand",
    "test:approve": "jest --runInBand --updateSnapshot",
    "pretty": "prettier --write \"**/*.ts\""
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "E-Vister",
  "license": "ISC",
  "devDependencies": {
    "@types/cheerio": "0.22.31",
    "@types/jest": "29.4.0",
    "@types/random-useragent": "0.3.1",
    "@types/request": "2.48.8",
    "@types/socket.io-client": "1.4.36",
    "@types/user-agents": "1.0.2",
    "@types/uuid": "8.3.4",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "request": "2.88.2",
    "socket.io-client": "2.4.0",
    "uuid": "8.3.2"
  }
}
